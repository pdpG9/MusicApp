<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="pasific_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/pacifico_regular</item>
    </style>

    <style name="railaway_bold_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/raleway_bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="railaway_medium_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/raleway_medium</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>